{
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    },
    {
      "path": "pages/axios/index",
      "style": {
        "navigationBarTitleText": "axios"
      }
    },
    {
      "path": "pages/vant/index",
      "style": {
        "navigationBarTitleText": "vant"
      }
    },
    {
      "path": "pages/vuex/index",
      "style": {
        "navigationBarTitleText": "vuex"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    // 此处引入vant-weapp组件
    "usingComponents": {
      "van-button": "/wxcomponents/@vant/weapp/dist/button/index",
      "van-icon": "/wxcomponents/@vant/weapp/dist/icon/index"
    }
  }
}
